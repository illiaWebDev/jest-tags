{
  "name": "@illia-web-dev/jest-tags",
  "version": "0.0.1",
  "description": "",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "husky:precommit": "npm run lint -- --max-warnings=0 && tsc --noEmit && NODE_ENV=DANGER_JEST_ONLY_NODE_ENV npm test",
    "transpileToDist": "rm -rf ./dist && tsc && rm -rf ./dist/*.test.*",
    "ts": "tsc --noEmit --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest -c=./jest.config.js",
    "prepare": "husky install",
    "publish-package:test:pack": "npm run transpileToDist && npm pack",
    "publish-package:test:unpack-archive": "tar zxvf",
    "publish-package:test:clean": "rm -rf ./package && rm -rf illia-web-dev-jest-tags*",
    "publish-package:publish": "npm run transpileToDist && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/illiaWebDev/jest-tags.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/illiaWebDev/jest-tags/issues"
  },
  "homepage": "https://github.com/illiaWebDev/jest-tags#readme",
  "peerDependencies": {
    "tslib": "^2",
    "typescript": "^4"
  },
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "jest": "29.5.0",
    "ts-jest": "29.1.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "8.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "husky": "8.0.3",
    "typescript": "5.1.3"
  }
}
